config_version=5

[application]

config/name="Роб"
config/tags=PackedStringArray("dangeon_party")
run/main_scene="res://scen/menu.tscn"
config/features=PackedStringArray("4.2", "Mobile")
run/max_fps=120
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://icon.svg"
boot_splash/minimum_display_time=100

[autoload]

SimpleGrass="*res://addons/simplegrasstextured/singleton.tscn"
Event="*res://scripts/event.gd"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/vsync/vsync_mode=2
window/bg_color=Color(0.14, 0.14, 0.14, 1)
window/name="Роб"

[dotnet]

project/assembly_name="Роб"

[editor_plugins]

enabled=PackedStringArray("res://addons/proton_scatter/plugin.cfg", "res://addons/simplegrasstextured/plugin.cfg", "res://addons/terrain_3d/plugin.cfg")

[filesystem]

import/fbx/enabled=false
import/blender/enabled.android=true

[importer_defaults]

animation_library={
"animation/fps": 120.0
}

